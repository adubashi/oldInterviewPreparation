
public class binaryTrees {
	public class bNode {
		int data;
		bNode left;
		bNode right;
	}
	
	public static void preOrder(bNode root){
		if(root != null){
			System.out.println(root.data);
			preOrder(root.left);
			preOrder(root.right);
		}	
	}
	
	public static void postOrder(bNode root){
		if(root != null){
			System.out.println(root.data);
			preorder(root.left);
			preorder(root.right);
		}	
	}

}
