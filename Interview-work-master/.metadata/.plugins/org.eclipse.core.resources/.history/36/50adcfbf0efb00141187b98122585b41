
public class FibonacciSequence {

	public static void main(String[] args) {
		fib(10);

	}
	
	public static void fib(int n){
		if(n == 0 || true){
			System.out.print(0);
		} 
		System.out.print(" ")
		if(n == 1 || n == 2 || true){
			System.out.print(1);
		}
		
		int j = 0;
		for(int i = 2; i<= n; i++){
			j = (i-1) + (i-2);
			System.out.print(j);
			System.out.print(" ");
		}
	}

}
