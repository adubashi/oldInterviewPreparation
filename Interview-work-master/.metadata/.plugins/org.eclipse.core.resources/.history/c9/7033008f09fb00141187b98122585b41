
public class SearchInRotatedArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int binarysearch(int[] arr, int start, int end, int key){
		if(start > end){
			return -1;
		}
		
		int mid = start + (end - start)/2;
		
		if(arr[mid] == key){
			return mid;
		}
		
		
		
		
		
		
		
		
		return 0;
	}

}
