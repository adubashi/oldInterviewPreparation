
public class FibonacciSequence {

	public static void main(String[] args) {
		fib(10)

	}
	
	public static void fib(int n){
		if(n == 0){
			System.out.print(0);
		} 
		if(n == 1 || n == 2){
			System.out.print(2);
		}
		
		int j = 0;
		for(int i = 3; i<= n; i++){
			j = (i-1) + (i-2);
			System.out.print(j);
		}
	}

}
